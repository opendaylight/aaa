<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

    <reference id="certConsoleProvider" availability="mandatory"
        activation="eager" interface="org.opendaylight.aaa.cert.ICertConsoleProvider">
    </reference>

    <command-bundle xmlns="http://karaf.apache.org/xmlns/shell/v1.1.0">
        <command>
            <action class="org.opendaylight.aaa.cli.CreateODLKeyStore">
                <argument ref="certConsoleProvider" />
            </action>
        </command>
    </command-bundle>

</blueprint>
